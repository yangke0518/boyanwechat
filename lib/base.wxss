@import "/component/hintstart/hintstart.wxss";
@import "/component/dotfooter/dotfooter.wxss";

image{
  display: block;
}
view,text,input,button{
  box-sizing: border-box;
}
.clearfix:after {
  visibility: hidden;
  display: block;
  font-size: 0;
  content: " ";
  clear: both;
  height: 0
}

.show{
  display: block !important;
}
.none{
  display: none !important;
}